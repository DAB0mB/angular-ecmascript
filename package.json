{
  "name": "angular-ecmascript",
  "description": "Write angular module-helpers using es6's new class system",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/DAB0mB/angular-ecmascript.git"
  },
  "scripts": {
    "build": "babel src/ -d dist/ --source-maps",
    "build:watch": "babel src/ -d dist/ --source-maps --watch",
    "test": "chromium-browser ./test/index.html",
    "test:build": "npm run build && webpack --config ./test/webpack.config.js",
    "test:build:watch": "npm run build:watch & webpack --config ./test/webpack.config.js --watch",
    "postinstall": "npm run build"
  },
  "peerDependencies": {
    "angular": "^1.5.0"
  },
  "devDependencies": {
    "angular": "^1.5.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "jasmine-core": "^2.4.0",
    "webpack": "^1.13.0"
  },
  "keywords": [
    "angular",
    "ecmascript",
    "angular-ecmascript"
  ]
}
